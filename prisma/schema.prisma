generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Slot {
  id        Int      @id @default(autoincrement())
  date      DateTime
  time      String
  chair     String
  status    String
  patient   String?  // nullable
  risk      String?  // nullable
  createdAt DateTime @default(now())
}
